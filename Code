public class OddEvenSort {
  public static String sortMyString(String s) {
    
    String even = s.charAt(0)+"";
    String odd = "";
    
    for(int i = 1; i<s.length(); i++){
      if(i%2==0){
        even+=s.charAt(i)+"";
      }else{
        odd+=s.charAt(i)+"";
      }
    }
    return even + " " + odd;
  }
}
